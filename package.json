{
	"name": "pepe-website",
	"private": true,
	"homepage": "https://pepemanager.com",
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"format": "prettier --write . && cross-env TIMING=1 eslint src --ext ts,tsx --fix",
		"fmt": "yarn format",
		"postinstall": "husky install .github/husky",
		"lint": "prettier --check . && cross-env TIMING=1 eslint src --ext ts,tsx",
		"start": "next start",
		"update": "yarn upgrade-interactive"
	},
	"lint-staged": {
		"*.tsx": "eslint --fix",
		"*.{ts,tsx,js,mjs,cjs,json,yml}": "prettier --write"
	},
	"resolutions": {
		"@angular-eslint/eslint-plugin": "npm:@almeidx/empty@1.0.1",
		"@angular-eslint/eslint-plugin-template": "npm:@almeidx/empty@1.0.1",
		"@angular-eslint/template-parser": "npm:@almeidx/empty@1.0.1",
		"astro-eslint-parser": "npm:@almeidx/empty@1.0.1",
		"eslint-plugin-astro": "npm:@almeidx/empty@1.0.1",
		"eslint-plugin-cypress": "npm:@almeidx/empty@1.0.1",
		"eslint-plugin-jest": "npm:@almeidx/empty@1.0.1",
		"eslint-plugin-rxjs": "npm:@almeidx/empty@1.0.1",
		"eslint-plugin-rxjs-angular": "npm:@almeidx/empty@1.0.1",
		"eslint-plugin-svelte3": "npm:@almeidx/empty@1.0.1",
		"eslint-plugin-vue": "npm:@almeidx/empty@1.0.1",
		"vue-eslint-parser": "npm:@almeidx/empty@1.0.1"
	},
	"dependencies": {
		"@almeidx/ms": "^1.2.1",
		"cuid": "^2.1.8",
		"js-cookie": "^3.0.1",
		"next": "^13.1.3",
		"next-progress": "^2.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.7.1"
	},
	"devDependencies": {
		"@types/gtag.js": "^0.0.12",
		"@types/js-cookie": "^3.0.2",
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.27",
		"autoprefixer": "^10.4.13",
		"cross-env": "^7.0.3",
		"eslint": "^8.32.0",
		"eslint-config-neon": "^0.1.40",
		"eslint-plugin-tailwindcss": "^3.8.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.3",
		"tailwindcss": "^3.2.4",
		"typescript": "~4.9.4"
	},
	"packageManager": "yarn@3.3.0",
	"engines": {
		"node": ">=18"
	}
}

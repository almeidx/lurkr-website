{
	"name": "pepe-website",
	"private": true,
	"homepage": "https://pepemanager.com",
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"format": "prettier --write . && cross-env TIMING=1 eslint src --ext ts,tsx --fix",
		"postinstall": "husky install .github/husky",
		"lint": "prettier --check . && cross-env TIMING=1 eslint src --ext ts,tsx",
		"start": "next start",
		"update": "yarn upgrade-interactive"
	},
	"lint-staged": {
		"*.tsx": "eslint --fix",
		"*.{ts,tsx,js,mjs,cjs,json,yml}": "prettier --write"
	},
	"dependencies": {
		"@almeidx/ms": "^1.2.1",
		"cuid": "^2.1.8",
		"js-cookie": "^3.0.1",
		"next": "^13.0.3",
		"next-progress": "^2.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.6.0"
	},
	"devDependencies": {
		"@types/gtag.js": "^0.0.12",
		"@types/js-cookie": "^3.0.2",
		"@types/node": "^16.18.3",
		"@types/react": "^18.0.25",
		"autoprefixer": "^10.4.13",
		"cross-env": "^7.0.3",
		"eslint": "^8.27.0",
		"eslint-config-neon": "^0.1.39",
		"eslint-plugin-tailwindcss": "^3.6.2",
		"husky": "^8.0.2",
		"lint-staged": "^13.0.3",
		"postcss": "^8.4.19",
		"prettier": "^2.7.1",
		"tailwindcss": "^3.2.3",
		"typescript": "~4.8.4"
	},
	"packageManager": "yarn@3.2.4",
	"engines": {
		"node": ">=16"
	}
}
